<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="@dimen/setting_switch_track_height"
                android:height="@dimen/setting_switch_thumb_width" />
            <solid android:color="#00FFFFFF" />
        </shape>
    </item>
    <item
        android:top="2dp"
        android:left="2dp"
        android:right="2dp"
        android:bottom="2dp"
        android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="@dimen/setting_switch_inner_thumb_width"
                android:height="@dimen/setting_switch_inner_thumb_width" />
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="2dp"
                android:color="@color/setting_switch" />
        </shape>
    </item>
</layer-list>